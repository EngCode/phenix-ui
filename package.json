{
  "name": "phenix-ui",
  "version": "0.2.0",
  "description": "Phenix Design System User Interface / Front-End Framework - Extended From 'Tornado UI v2'.",
  "author": "Abdullah.Ramadan <elampratorabdallah14@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EngCode/phenix-ui"
  },
  "bugs": {
    "url": "https://github.com/EngCode/phenix-ui/issues"
  },
  "homepage": "https://design.phenixthemes.com",
  "keywords": [
    "phenix",
    "phenix ui",
    "tornado ui",
    "design system",
    "bootstrap",
    "tailwind",
    "framework",
    "css",
    "js",
    "esNext",
    "web",
    "app",
    "enviroment",
    "webpack",
    "sass",
    "rtl",
    "typescript"
  ],
  "main": "/dist/css/phenix.css",
  "exports": {
    "phenix-ts": "./src/typescript/",
    "phenix-sass": "./src/sass/",
    "phenix-css": "./dist/css/",
    "phenix-js": "./dist/js/"
  },
  "scripts": {
    "phenix-ts": "npx webpack --config typescript.webpack.js --watch",
    "phenix-sass": "npx node-sass --watch src/sass -o dist/css --output-style compressed --source-map true",
    "phenix-start": "concurrently --kill-others \"npm run phenix-sass\" \"npm run phenix-ts\""
  },
  "devDependencies": {},
  "dependencies": {
    "@splidejs/splide": "^3.5.0",
    "concurrently": "^6.4.0",
    "node-sass": "^6.0.1",
    "ts-loader": "^7.0.5",
    "typescript": "^3.9.7",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.5.0"
  }
}
